<!DOCTYPE html >
<html>
	<head>
		<meta charset="UTF-8">
		<title>Insert title here</title>
	</head>
	

	<body>
		forEach.jsp
		<hr/>

		<c:forEach var="i" begin="0" end="9">
			${ i} <br/>
		</c:forEach>
		<hr/>
		
		
		<c:forEach var="i" begin="0" end="9" step="2">
			${ i} <br/>
		</c:forEach>
		
		<hr/>
		<c:forEach var="i" begin="11" end="15" varStatus="status">
			status: ${status.count}, i:${i} <br/>
		</c:forEach>
	
	</body>
